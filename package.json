    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^10.0.0",
    "budo": "^8.2.0",
    "gh-pages": "^0.3.0",
